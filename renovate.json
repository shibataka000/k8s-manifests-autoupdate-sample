{
    "repositories": [
        "shibataka000/renovate-sample"
    ],
    "extends": [
        "config:base",
        ":prHourlyLimitNone",
        ":prConcurrentLimitNone"
    ],
    "enabledManagers": [
        "dockerfile",
        "kustomize"
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "^.*kustomization.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/raw\\.githubusercontent\\.com\\/(?<depName>[^/]+\\/[^/]+)\\/(?<currentValue>[^/]+)\\/.*\\s+#\\s*renovate:enabled"
            ],
            "datasourceTemplate": "github-releases",
            "versioningTemplate": "semver"
        }
    ]
}
